<div id="screen-{{_screenID}}" class="action-container" aria-label="{{{title}}}">
  {{#each _childItems}}
    {{#if type.input}}
    <input data-id="{{id}}" type="text" class="input-zone simulation-action-element"
      style="width:{{_position._width}}%;height:{{_position._height}}%;top:{{_position._top}}%;left:{{_position._left}}%;" />
    {{/if}}
    {{#if type.select}}
    <select data-id="{{id}}" name="options" id="options" class="select-zone simulation-action-element"
      style="width:{{_position._width}}%;height:{{_position._height}}%;top:{{_position._top}}%;left:{{_position._left}}%;">
      {{#each _selectOptions}}
      <option value="{{_selectValue}}">{{_selectValue}}</option>
      {{/each}}
    </select>
    {{/if}}
    {{#if type.click}}
    <div data-id="{{id}}" role="button" tabindex="0" aria-label="{{title}}" class="click-zone simulation-action-element"
      style="width:{{_position._width}}%;height:{{_position._height}}%;top:{{_position._top}}%;left:{{_position._left}}%;">
    </div>
    {{/if}}
  {{/each}}
</div>
